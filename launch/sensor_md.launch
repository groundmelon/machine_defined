<launch>  
    <node pkg="bluefox2" type="bluefox2" name="bluefox2" output="screen" launch-prefix="">
        <param name="fps" type="double" value="30.0" />
        <param name="gain" type="double" value="0.0" />
        <param name="use_auto_exposure" type="bool" value="false" />
        <param name="cam_cnt" type="int" value="1" />
        <param name="seriala" type="string" value="25001285" />   
        <param name="exposure_time_usa" type="int" value="10000" />     
        <param name="serialb" type="string" value="25000709" />   
        <param name="exposure_time_usb" type="int" value="10000" />     
        <param name="serialc" type="string" value="25001161" />   
        <param name="exposure_time_usc" type="int" value="10000" />     
        <param name="pub_cnt" type="int" value="1" />   
        <param name="mask0" type="string" value="a" />
        <remap from="~a" to="~image" />
    </node> 
	
    <node pkg="djiros" type="djiros_node" name="djiros" output="screen"
		launch-prefix="" >
		<param name="serial_name" type="string" value="/dev/ttyUSB0"/>
		<param name="baud_rate" type="int" value="230400"/>
		<param name="app_id" type="int" value="$(env DJIROS_APPID)"/>
		<param name="app_version" type="int" value="1"/>
		<param name="enc_key" type="string" value="$(env DJIROS_ENCKEY)"/>	
		<param name="only_broadcast" type="bool" value="false"/>
		<param name="align_with_fmu" type="bool" value="false"/>
	</node>

</launch>
