<launch>
    <node pkg="djiros" type="djifox_node" name="djiros" output="screen" launch-prefix="">
        <param name="serial_name" type="string" value="/dev/ttyUSB0"/>
        <param name="baud_rate" type="int" value="921600"/>
        <param name="A3_or_M100" type="int" value="0"/>
        <param name="app_id" type="int" value="$(env DJIROS_APPID)"/>
        <param name="app_version" type="int" value="1"/>
        <param name="enc_key" type="string" value="$(env DJIROS_ENCKEY)"/>  
        <param name="only_broadcast" type="bool" value="false"/>
        <param name="use_imu_filter" type="bool" value="true"/>
        <param name="align_with_fmu" type="bool" value="false"/>
        <rosparam>
            # common setting and default individual settings
            verbose_output: false
            cam_cnt: 1
            fps:  20
            gain: 1.0
            use_auto_exposure: false
            fast_mode: false

            # individual setting for camera 0
            camera0:
                serial: "25000049"
                topic: image
                use_auto_exposure: false
                exposure_time_us: 10000
                auto_speed: 0 # 0--1--2, slow--medium--fast converge to desired value
                gain: 1.0
                is_slave: true
        </rosparam>
        <remap from="~imu_filtered" to="~imu" />
    </node> 
    
    <include if="true"
        file="$(find imu_3dm_gx4)/launch/imu.launch" >
        <arg name="imu_decimation" value="5" />
        <arg name="filter_decimation" value="2" />
        <arg name="enable_filter" default="true"/>
    </include>
    
    <node if="false" pkg="terarangerone" type="terarangerone_node" name="terarangerone" output="screen">
		<param name="portname" value="/dev/ttyUSBTerarangerOne" />
	</node>

    <node if="false" pkg="sonarros" type="maxsonar.py" name="sonar" output="screen">
        <param name="devname" type="string" value="/dev/ttyUSB0"/>
        <param name="baudrate" type="int" value="9600"/>
        <rosparam>
            limit_low      : 0.21
            limit_high     : 5.0 
            install_height : 0.17
        </rosparam>
    </node>

    <node if="false"
    	pkg="bluefox2" type="bluefox2" name="bluefox2" output="screen">
        <param name="fps" type="double" value="30.0" />
        <param name="gain" type="double" value="0.0" />
        <param name="use_auto_exposure" type="bool" value="false" />
        <param name="cam_cnt" type="int" value="1" />
        <param name="seriala" type="string" value="25000681" />   
        <param name="exposure_time_usa" type="int" value="5000" />     
        <param name="serialb" type="string" value="25000709" />   
        <param name="exposure_time_usb" type="int" value="10000" />     
        <param name="serialc" type="string" value="25001161" />   
        <param name="exposure_time_usc" type="int" value="10000" />     
        <param name="pub_cnt" type="int" value="1" />   
        <param name="mask0" type="string" value="a" />
        <remap from="~a" to="~image" />
    </node> 
</launch>
