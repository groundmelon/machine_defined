<launch>
	<include file="$(find uavlaunch)/launch/ctrl.launch" >
		<arg name="uavname" default="$(env UAVNAME)" />
		<arg name="enable_djisim" default="false" />
		<arg name="use_jchen" default="true" />

		<arg name="waypoint_type" value="manual"/>
		<arg name="traj_vel" value="5.0"/>
		<arg name="traj_acc" value="4.0"/>
	</include>
</launch>
